{
    "name": "Tools EOP",
    "manifest_version": 2,
    "version": "1.0",
    "description": "Day la tien ich viet cho vui",
    "author": "Luu Minh",
    "permissions": [
        "tabs",
        "http://eop.edu.vn/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://eop.edu.vn/study/task/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "browser_action": {
        "default_title": "Tool auto click in eop",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}